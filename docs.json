{
    "$schema": "https://mintlify.com/docs.json",
    "theme": "mint",
    "name": "CozyCube",
    "colors": {
        "primary": "#16A34A",
        "light": "#07C983",
        "dark": "#15803D"
    },
    "favicon": "/favicon.svg",
    "navigation": {
        "tabs": [
            {
                "tab": "Guides",
                "groups": [
                    {
                        "group": "Get Started",
                        "pages": [
                            "index",
                            "quickstart",
                            "development"
                        ]
                    },
                    {
                        "group": "Essentials",
                        "pages": [
                            "essentials/markdown",
                            "essentials/code",
                            "essentials/images",
                            "essentials/settings",
                            "essentials/navigation",
                            "essentials/reusable-snippets"
                        ]
                    }
                ]
            }
        ],
        "global": {
            "anchors": [
                {
                    "anchor": "Discord",
                    "href": "https://discord.cozycube.fr",
                    "icon": "message-circle-more"
                }
            ]
        }
    },
    "logo": {
        "light": "/logo/light.svg",
        "dark": "/logo/dark.svg"
    },
    "navbar": {
        "links": [
            {
                "label": "Discord",
                "href": "https://discord.cozycube.fr"
            }
        ],
        "primary": {
            "type": "button",
            "label": "Dashboard",
            "href": "https://dashboard.mintlify.com"
        }
    }
}